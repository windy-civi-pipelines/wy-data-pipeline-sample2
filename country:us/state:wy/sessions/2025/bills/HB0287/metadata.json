{
  "legislative_session": "2025",
  "identifier": "HB0287",
  "title": "Protecting self-defense-reimbursement and amendments.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [],
  "other_titles": [
    {
      "note": "",
      "title": "AN ACT relating to crimes and offenses; clarifying the use of self-defense to prevent injury or loss to other persons and property; requiring the expungement of records for cases dismissed for use of self-defense; authorizing reimbursement of costs incurred in defending a criminal prosecution after successfully asserting self-defense; providing definitions; making conforming amendments; specifying applicability; and providing for an effective date."
    }
  ],
