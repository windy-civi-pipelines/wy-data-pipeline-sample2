  "legislative_session": "2025",
  "identifier": "HB0337",
  "title": "Prohibiting foreign funding of ballot measures.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "summary",
