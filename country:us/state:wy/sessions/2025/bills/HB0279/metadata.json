  "other_titles": [
    {
      "note": "",
      "title": "AN ACT relating to property taxation; specifying that if multiple property tax exemptions apply to the same property the exemptions shall be applied in the manner determined by the department of revenue; specifying requirements for the order of application of exemptions; providing an appropriation; and providing for an effective date."
    }
  ],
