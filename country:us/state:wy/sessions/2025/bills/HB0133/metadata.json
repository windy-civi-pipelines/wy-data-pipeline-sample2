  "other_titles": [
    {
      "note": "",
      "title": "AN ACT relating to administration of the government; prohibiting the implementation of immigration sanctuary policies by cities, towns, counties and the state; prohibiting the designation of sanctuary cities, towns, counties and the state; creating a new felony offense; providing penalties; providing definitions; making conforming amendments; and providing for an effective date."
    }
  ],
