{
  "legislative_session": "2025",
  "identifier": "HB0156",
  "title": "Proof of voter residency-registration qualifications.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "summary",
      "abstract": "2025/Summaries/HB0156.pdf"
    }
  ],
  "other_titles": [
    {
      "note": "",
      "title": "AN ACT relating to elections; requiring a qualified elector to be a bona fide resident of the state of Wyoming for not less than thirty (30) days before the date of the election in which they offer to vote; requiring documentation proving residence to register to vote; prohibiting registration based on documentation showing noncitizen status; making conforming amendments; requiring rulemaking; and providing for effective dates."
    }
  ],
