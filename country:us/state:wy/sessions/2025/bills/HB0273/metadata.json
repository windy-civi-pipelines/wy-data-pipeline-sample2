  "identifier": "HB0273",
  "title": "Wyoming pregnancy centers-autonomy and rights.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [],
  "other_titles": [
    {
      "note": "",
      "title": "AN ACT relating to public health and safety; providing legislative findings; prohibiting the state and specified governmental entities from adopting any law, rule or policy that targets pregnancy centers for oversight or regulation based on the centers' stance against abortion; providing definitions; providing penalties; specifying available damages; providing a right of intervention for members of the legislature as specified; making conforming amendments; and providing for an effective date."
    }
  ],
